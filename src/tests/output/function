; ModuleID = 'module'
source_filename = "module"

define i32 @half(i32 %0) {
entry:
  %x = alloca i32, align 4
  store i32 %0, ptr %x, align 4
  %x1 = load i32, ptr %x, align 4
  %int_div = sdiv i32 %x1, 2
  ret i32 %int_div
}

define void @f() {
entry:
}

define i32 @main() {
entry:
  call void @f()
  %half = call i32 @half(i32 10)
  ret i32 %half
}
