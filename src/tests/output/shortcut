; ModuleID = 'module'
source_filename = "module"

define i32 @main() {
entry:
  %a = alloca i32, align 4
  store i32 0, ptr %a, align 4
  %b = alloca i32, align 4
  store i32 1, ptr %b, align 4
  %a1 = load i32, ptr %a, align 4
  %b2 = load i32, ptr %b, align 4
  %logical_or = or i32 %a1, %b2
  br i32 %logical_or, label %if_block, label %after_block

if_block:                                         ; preds = %entry
  %a3 = load i32, ptr %a, align 4
  %b4 = load i32, ptr %b, align 4
  %int_add = add i32 %a3, %b4
  store i32 %int_add, ptr %a, align 4
  br label %after_block

after_block:                                      ; preds = %if_block, %entry
  %a5 = load i32, ptr %a, align 4
  ret i32 %a5
}
